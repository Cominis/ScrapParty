{
  "manifest_version": 2,
  "name": "Lost and found",
  "description": "Extension to find missing party in google meet",
  "version": "1.0",
  "page_action": {
    "default_icon": {
      "16": "images/logo.png",
      "24": "images/logo.png",
      "32": "images/logo.png"
    },
    "default_title": "Lost and found",
    "default_popup": "index.html"
  },
  "permissions": ["activeTab", "tabs"],
  "icons": {
    "16": "images/logo.png",
    "48": "images/logo.png",
    "128": "images/logo.png"
  },
  "background": {
    "scripts": ["js/enable.js"]
  }
}
