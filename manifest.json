{
  "manifest_version": 2,
  "name": "Lost and found",
  "description": "Extension to find missing party in google meet",
  "version": "1.0",
  "page_action": {
    "default_icon": {
      "16": "images/logo16.png",
      "24": "images/logo24.png",
      "32": "images/logo32.png",
      "48": "images/logo48.png",
      "128": "images/logo128.png"
    },
    "default_title": "Lost and found",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://lost-and-found-node.herokuapp.com/*"
  ],
  "icons": {
    "16": "images/logo16.png",
    "24": "images/logo24.png",
    "32": "images/logo32.png",
    "48": "images/logo48.png",
    "128": "images/logo128.png"
  },
  "background": {
    "scripts": ["js/enable.js"]
  }
}
